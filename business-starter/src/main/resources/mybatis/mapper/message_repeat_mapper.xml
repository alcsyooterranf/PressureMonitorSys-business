<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.pms.infrastructure.mapper.IMessageRepeatMapper">
    <insert id="insert" parameterType="String">
        INSERT INTO pressuremonitorsys.t_message_repeat(`key`)
        VALUES (#{key})
    </insert>

    <delete id="deleteByKey">
        DELETE
        FROM pressuremonitorsys.t_message_repeat
        WHERE `key` = #{key}
    </delete>
</mapper>
